{
  "name": "objection-graphql-resolver",
  "type": "module",
  "version": "7.6.0",
  "description": "objection.js plugin: resolve GraphQL query using withGraphFetched",
  "author": "Ilya Semenov",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/IlyaSemenov/objection-graphql-resolver.git"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "main": "dist/index.cjs",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "dev": "vite-node --watch playground/readme.sample.ts",
    "build": "tsup",
    "test": "vitest run && tsc --noEmit",
    "prepublishOnly": "pnpm build"
  },
  "peerDependencies": {
    "graphql": "^16",
    "objection": "^2 || ^3"
  },
  "dependencies": {
    "graphql-parse-resolve-info": "^4.13.0"
  },
  "devDependencies": {
    "@apollo/server": "^4.7.1",
    "graphql": "^16.6.0",
    "graphql-request": "^6.0.0",
    "graphql-tag": "^2.12.6",
    "knex": "^2.4.2",
    "objection": "^3.0.1",
    "sqlite3": "^5.1.6",
    "tar": "^6.1.15",
    "tsconfig-vite-node": "^1.1.2",
    "tsup": "^8.5.0",
    "vite": "^7.0.5",
    "vite-node": "^3.2.4",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4"
  }
}
