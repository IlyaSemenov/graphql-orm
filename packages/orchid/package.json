{
  "name": "orchid-graphql",
  "type": "module",
  "version": "1.8.1",
  "description": "orchid-orm plugin: resolve GraphQL query with Orchid ORM",
  "author": "Ilya Semenov",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/IlyaSemenov/orchid-graphql"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "main": "dist/index.cjs",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "dev": "vite-node --watch playground/readme.sample.ts",
    "build": "tsup",
    "test": "vitest run && tsc --noEmit && tstyche",
    "prepublishOnly": "pnpm build"
  },
  "peerDependencies": {
    "graphql": "^16",
    "orchid-orm": "^1.31.5"
  },
  "dependencies": {
    "graphql-parse-resolve-info": "^4.13.0"
  },
  "devDependencies": {
    "@apollo/server": "^4.7.1",
    "graphql": "^16.6.0",
    "graphql-orm": "workspace:*",
    "graphql-request": "^6.0.0",
    "graphql-tag": "^2.12.6",
    "orchid-orm": "^1.31.5",
    "tsconfig-vite-node": "^1.1.2",
    "tstyche": "^4.3.0",
    "tsup": "^8.5.0",
    "vite": "^7.0.5",
    "vite-node": "^3.2.4",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4"
  }
}
