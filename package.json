{
  "name": "objection-graphql-resolver",
  "description": "objection.js plugin: resolve GraphQL query using withGraphFetched",
  "version": "6.0.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/IlyaSemenov/objection-graphql-resolver.git"
  },
  "author": "Ilya Semenov",
  "license": "MIT",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "source": "src/index.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "dev": "vite-node --watch playground/readme.sample.ts",
    "build": "tsup",
    "lint": "eslint --fix '**/*.{cjs,js,ts}'",
    "prepack": "npm run build",
    "prepare": "husky install",
    "test": "vitest run",
    "release": "npm run prepack && release-it"
  },
  "dependencies": {
    "graphql-parse-resolve-info": "^4.12.0"
  },
  "peerDependencies": {
    "objection": "^2 || ^3"
  },
  "devDependencies": {
    "@apollo/server": "~4.5.0",
    "@commitlint/cli": "~17.5.0",
    "@commitlint/config-conventional": "~17.4.4",
    "@tsconfig/node14": "~1.0.3",
    "@typescript-eslint/eslint-plugin": "~5.56.0",
    "@typescript-eslint/parser": "~5.56.0",
    "eslint": "~8.36.0",
    "eslint-config-prettier": "~8.8.0",
    "eslint-plugin-prettier": "~4.2.1",
    "eslint-plugin-simple-import-sort": "~10.0.0",
    "graphql": "~16.6.0",
    "graphql-request": "~5.2.0",
    "graphql-tag": "^2.11.0",
    "husky": "~8.0.3",
    "knex": "~2.4.2",
    "lint-staged": "~13.2.0",
    "objection": "^3.0.1",
    "prettier": "~2.8.7",
    "release-it": "~15.10.3",
    "sqlite3": "~5.1.6",
    "tar": "~6.1.13",
    "tsup": "~6.7.0",
    "typescript": "~5.0.2",
    "vite": "~4.3.8",
    "vite-node": "~0.31.1",
    "vite-tsconfig-paths": "~4.2.0",
    "vitest": "~0.31.1"
  }
}
